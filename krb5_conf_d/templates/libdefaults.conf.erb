[libdefaults]
<%- if @dns_lookup_realm -%>
  dns_lookup_realm = <%= @dns_lookup_realm %>
<%- end -%>
<%- if @dns_lookup_kdc -%>
  dns_lookup_kdc = <%= @dns_lookup_kdc %>
<%- end -%>
<%- if @rdns -%>
  rdns = <%= @rdns %>
<%- end -%>
<%- if @dns_canonicalize_hostname -%>
  dns_canonicalize_hostname = <%= @dns_canonicalize_hostname %>
<%- end -%>
<%- if @ticket_lifetime -%>
  ticket_lifetime = <%= @ticket_lifetime %>
<%- end -%>
<%- if @renew_lifetime -%>
  renew_lifetime = <%= @renew_lifetime %>
<%- end -%>
<%- if @udp_preference_limit -%>
  udp_preference_limit = <%= @udp_preference_limit %>
<%- end -%>
<%- if @allow_weak_crypto -%>
  allow_weak_crypto = <%= @allow_weak_crypto %>
<%- end -%>

